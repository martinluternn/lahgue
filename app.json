{
  "expo": {
    "name": "lahgue",
    "slug": "lahgue",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/ic-logo.jpeg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/ic-logo.jpeg",
      "resizeMode": "contain",
      "backgroundColor": "#65a4ec"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/ic-logo.jpeg",
        "backgroundColor": "#65a4ec"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/ic-logo.jpeg"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
